  },
  "homepage": "https://github.com/fitsumlegesse/NuGebeya#readme",
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.12.5"
  }
}
